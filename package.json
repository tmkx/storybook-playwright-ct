{
  "name": "storybook-playwright-ct",
  "version": "0.0.1",
  "description": "Storybook <3 Playwright",
  "author": "Tmk <i@tmk.im>",
  "keywords": [
    "storybook",
    "playwright",
    "e2e"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": {
    "url": "https://github.com/tmkx/storybook-playwright-ct.git"
  },
  "scripts": {
